@model List<AppUser>;

@{
    ViewData["Title"] = "TUserList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>TUserList</h1>
<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Oyuncu İsmi</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name @item.SurName</td>
            </tr>
        }
    </tbody>
</table>
